{
  "expo": {
    "name": "SimCareerApp",
    "slug": "SimCareerApp",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/img/icon.png",
    "userInterfaceStyle": "automatic",
    "notification": {
      "androidMode": "default",
      "icon": "./assets/logo96x96.png",
      "color": "#3366ff"
    },
    "splash": {
      "image": "./assets/img/splash/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#23A4ED"
    },
    "android": {
      "useNextNotificationsApi": true,
      "userInterfaceStyle": "dark",
      "adaptiveIcon": {
        "foregroundImage": "./assets/img/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      }
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true,
      "userInterfaceStyle": "light"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
}
